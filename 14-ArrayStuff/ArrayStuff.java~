public class ArrayStuff{
    public int find(int[] ray, int n){
	int count = 0;
	int result = -1;
	while (n < ray.length){
	    if (int[count] = n) {
		result = count;
		count = ray.length;
	    } else {
		count = count + 1;
	    }
	}
	return result;
    }

    public int maxVal(int[] ray){
	int count = 1;
	int stalin = int[0];
	while (count < ray.length){
	    if (int[count] > stalin) {
		stalin = int[count];
		count = count + 1;
	    } else {
		count = count + 1;
	    }
	}
	return stalin;
    }
}
